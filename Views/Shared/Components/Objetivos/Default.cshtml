@model IEnumerable<MontagemCurriculo.Models.Objetivo>

@foreach (var item in Model)
{
    <li class="collection-item row s10">
        <div class="valign-wrapper col s10">
            @Html.DisplayFor(ModelItem => item.Descrição)
        </div>

        <div class="col s2 right">
                      

                <a asp-controller="Objetivos" asp-action="Edit" asp-route-id="@item.ObjetivoID" class="btn-floating btn-small blue darken-4">
                    <i class="material-icons">edit</i>
                </a>
                <a onclick="ExibirModal(@item.ObjetivoID, 'objetivos')" class="btn-floating btn-small red darken-4">
                    <i class="material-icons">delete_forever</i>
                </a>
           

        </div>
    </li>
}